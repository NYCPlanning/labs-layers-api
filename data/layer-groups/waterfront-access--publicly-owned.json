{
  "id": "waterfront-access--publicly-owned",
  "legend": {
    "label": "Publicly Owned Spaces",
    "tooltip": "Government owned and operated publicly accessible open space",
    "icon": {
      "type": "rectangle",
      "layers": [
        {
          "fill": "rgba(154, 180, 100, 1)",
          "stroke": "rgba(154, 180, 100, 0.6)",
          "stroke-width": "2"
        }
      ]
    }
  },
  "layers": [
    {
      "before": "building",
      "style": {
        "id": "publicly-owned-waterfront-fill",
        "type": "fill",
        "source": "paws",
        "source-layer": "publiclyownedwaterfront",
        "paint": {
          "fill-color": "rgba(154, 180, 100, 1)"
        }
      }
    },
    {
      "style": {
        "id": "publicly-owned-waterfront-overlay",
        "type": "fill",
        "source": "paws",
        "source-layer": "publiclyownedwaterfront",
        "paint": {
          "fill-color": "rgba(154, 180, 100, 0.7)"
        }
      },
      "highlightable": true,
      "clickable": true,
      "tooltipable": true,
      "tooltipTemplate": "{{{park_name}}}"
    },
    {
      "style": {
        "id": "publicly-owned-waterfront-halo",
        "type": "line",
        "source": "paws",
        "source-layer": "publiclyownedwaterfront",
        "paint": {
          "line-color": "rgba(154, 180, 100, 1)",
          "line-opacity": {
            "stops": [
              [10, 1],
              [13, 0.3]
            ]
          },
          "line-width": {
            "stops": [
              [12, 4],
              [14, 0]
            ]
          }
        },
        "layout": {
          "line-cap": "round",
          "line-join": "round"
        }
      }
    }
  ]
}
