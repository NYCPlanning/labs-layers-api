{
  "id": "factfinder--cdtas",
  "layers": [
    {
      "style": {
        "id": "factfinder--cdtas-fill",
        "type": "fill",
        "source": "factfinder--admin-boundaries",
        "source-layer": "cdtas",
        "paint": {
          "fill-opacity": 0.01
        }
      },
      "highlightable": true,
      "clickable": true
    },
    {
      "style": {
        "id": "cdtas-line",
        "type": "line",
        "source": "factfinder--admin-boundaries",
        "source-layer": "cdtas",
        "paint": {
          "line-color": "#444",
          "line-opacity": 0.5,
          "line-width": {
            "stops": [
              [
                11,
                1
              ],
              [
                16,
                3
              ]
            ]
          }
        },
        "layout": {
          "line-join": "round",
          "line-cap": "round"
        }
      }
    },
    {
      "style": {
        "id": "cdtas-label",
        "type": "symbol",
        "source": "factfinder--admin-boundaries",
        "source-layer": "cdta-centroids",
        "minzoom": 0,
        "paint": {
          "text-color": [
            "interpolate",
            ["linear"],
            ["zoom"],
            12,
            "#d3d3d3",
            13,
            "#626262"
          ],
          "text-halo-color": "#FFFFFF",
          "text-halo-width": 2,
          "text-halo-blur": 2
        },
        "layout": {
          "text-field": "{cdtaname}",
          "text-font": [
            "Open Sans Semibold",
            "Arial Unicode MS Bold"
          ],
          "text-allow-overlap": true,
          "text-size": {
            "stops": [
              [
                11,
                12
              ],
              [
                14,
                16
              ]
            ]
          }
        }
      }
    }
  ]
}
