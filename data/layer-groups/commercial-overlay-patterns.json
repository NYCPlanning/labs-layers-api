{
  "id": "commercial-overlay-patterns",
  "legend": {
    "label": "Commercial Overlays",
    "tooltip": "A commercial overlay is a C1 or C2 district mapped within residential districts to serve local retail needs."
  },
  "layers": [
    {
      "style": {
        "id": "c1-1",
        "type": "fill",
        "source": "supporting-zoning",
        "source-layer": "commercial-overlays",
        "paint": {"fill-pattern": "black-45"},
        "filter": ["all",["==","overlay","C1-1"]]
      }
    },
    {
      "style": {
        "id": "c1-2",
        "type": "fill",
        "source": "supporting-zoning",
        "source-layer": "commercial-overlays",
        "paint": {"fill-pattern": "black-45-135"},
        "filter": ["all",["==","overlay","C1-2"]]
      }
    },
    {
      "style": {
        "id": "c1-3",
        "type": "fill",
        "source": "supporting-zoning",
        "source-layer": "commercial-overlays",
        "paint": {"fill-pattern": "black-45_altbold"},
        "filter": ["all",["==","overlay","C1-3"]]
      }
    },
    {
      "style": {
        "id": "c1-4",
        "type": "fill",
        "source": "supporting-zoning",
        "source-layer": "commercial-overlays",
        "paint": {"fill-pattern": "black-45_altbold-135_altbold"},
        "filter": ["all",["==","overlay","C1-4"]]
      }
    },
    {
      "style": {
        "id": "c1-5",
        "type": "fill",
        "source": "supporting-zoning",
        "source-layer": "commercial-overlays",
        "paint": {"fill-pattern": "black-45_dash"},
        "filter": ["all",["==","overlay","C1-5"]]
      }
    },
    {
      "style": {
        "id": "c2-1",
        "type": "fill",
        "source": "supporting-zoning",
        "source-layer": "commercial-overlays",
        "paint": {"fill-pattern": "black-45-dot"},
        "filter": ["all",["==","overlay","C2-1"]]
      }
    },
    {
      "style": {
        "id": "c2-2",
        "type": "fill",
        "source": "supporting-zoning",
        "source-layer": "commercial-overlays",
        "paint": {"fill-pattern": "black-45-135-dot"},
        "filter": ["all",["==","overlay","C2-2"]]
      }
    },
    {
      "style": {
        "id": "c2-3",
        "type": "fill",
        "source": "supporting-zoning",
        "source-layer": "commercial-overlays",
        "paint": {"fill-pattern": "black-45_altbold-dot"},
        "filter": ["all",["==","overlay","C2-3"]]
      }
    },
    {
      "style": {
        "id": "c2-4",
        "type": "fill",
        "source": "supporting-zoning",
        "source-layer": "commercial-overlays",
        "paint": {"fill-pattern": "black-45_altbold-135_altbold-dot"},
        "filter": ["all",["==","overlay","C2-4"]]
      }
    },
    {
      "style": {
        "id": "c2-5",
        "type": "fill",
        "source": "supporting-zoning",
        "source-layer": "commercial-overlays",
        "paint": {"fill-pattern": "black-45_dash-dot"},
        "filter": ["all",["==","overlay","C2-5"]]
      }
    },
    {
      "style": {
        "id": "co_outline",
        "type": "line",
        "source": "supporting-zoning",
        "source-layer": "commercial-overlays",
        "paint": {
          "line-width": 1,
          "line-color": "rgba(33, 35, 38, 1)"
        }
      }
    },
    {
      "before": "place_country_major",
      "style": {
        "id": "co_labels",
        "type": "symbol",
        "source": "supporting-zoning",
        "source-layer": "commercial-overlays",
        "paint": {
          "text-color": "rgba(33, 35, 38, 1)",
          "text-halo-color": "#fafafa",
          "text-halo-width": 2.5,
          "text-halo-blur": 1.5
        },
        "layout": {
          "symbol-placement": "point",
          "text-size": {
            "stops": [
              [
                14,
                8
              ],
              [
                18,
                16
              ]
            ]
          },
          "text-font": ["Open Sans Bold","Arial Unicode MS Bold"],
          "text-field": "{overlay}"
        }
      }
    }
  ]
}
